{
  "name": "excali",
  "private": true,
  "version": "1.0.3-2",
  "scripts": {
    "page:dev": "bun --filter ./packages/excali-page dev",
    "page:build": "BUILD_FOR_EXTENSION=1 bun --filter ./packages/excali-page build",
    "local:dev": "bun --filter ./packages/excali-local dev",
    "local:build": "bun --filter ./packages/excali-local build",
    "local:clean:archive": "bun --filter ./packages/excali-local clean:archive",
    "local:tar": "bun --filter ./packages/excali-local tar"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {}
}
